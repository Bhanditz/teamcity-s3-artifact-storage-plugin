<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>s3-artifact-storage</name>
    <display-name>AWS S3 Artifact Storage</display-name>
    <version>@Version@</version>
    <description>Allows storing build artifacts in an AWS S3 bucket</description>
    <download-url>https://github.com/JetBrains/teamcity-s3-artifact-storage-plugin</download-url>
    <vendor>
      <name>@VendorName@</name>
      <url>@VendorURL@</url>
    </vendor>
  </info>
  <requirements min-build="46654" max-build="57000" />
  <deployment use-separate-classloader="true" />
</teamcity-plugin>
